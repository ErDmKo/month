{
    "$schema": "http://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "target": "esNext",
        "alwaysStrict": false,
        "strict": true,
        "module": "ES2022",
        "moduleResolution": "node",
        "noEmit": false,
        "skipLibCheck": true,
        "declaration": true,
        "esModuleInterop": false,
        "lib": [
            "es2017",
            "dom"
        ],
        "types": [
            "node"
        ],
        "typeRoots": [
            "@types",
            "./node_modules/@types"
        ],
        "baseUrl": ".",
        "paths": {
            "@month/*": [
                "./assets/js/*",
                "./bazel-out/darwin-fastbuild/bin/assets/js/*",
                "./bazel-out/darwin_arm64-fastbuild/bin/assets/js/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}
